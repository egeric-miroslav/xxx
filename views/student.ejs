    <img src="img/logo.png" alt="">
    <p class="lead">Kriminalistiƒçko Policijski Univerzitet</p>
    <hr>

    <% if(aktivnosti && aktivnosti.length > 0) { %>
    <table>
      <tr>
        <th><%= %></th>
        <th>Broj indeksa</th>
        <th>Ime i prezime</th>
        <th>Aktivnost</th>
        <th>Vreme</th>
        <th>Napomena</th>
      </tr>
      <% for(var i = 0; i < aktivnosti.length; i++){ %>
